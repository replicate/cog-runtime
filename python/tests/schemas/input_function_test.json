            "nullable": true
          },
          "webhook": {
            "title": "Webhook",
            "type": "string",
            "maxLength": 65536,
            "minLength": 1,
            "format": "uri",
            "nullable": true
          },
          "webhook_events_filter": {
            "default": [
              "start",
              "output",
              "logs",
              "completed"
