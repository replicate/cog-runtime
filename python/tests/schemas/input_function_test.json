            "nullable": true
          },
          "metrics": {
            "title": "Metrics",
            "type": "object",
            "additionalProperties": true,
            "nullable": true
          }
        },
        "type": "object",
        "title": "PredictionResponse"
      },
      "Status": {
        "type": "string",
        "enum": [
          "starting",
          "processing",
          "succeeded",
          "canceled",
          "failed"
        ],
        "title": "Status",
        "description": "An enumeration."
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "type": "array",
            "title": "Location"
          },
          "msg": {
            "type": "string",
